{
    "name": "Getting Started Example",
    "description": "Build an Extension!",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/images/Leaf16.png",
            "32": "/images/Leaf32.png",
            "48": "/images/Leaf48.png",
            "128": "/images/Leaf128.png"
        }
    },
    "icons": {
        "16": "/images/Leaf16.png",
        "32": "/images/Leaf32.png",
        "48": "/images/Leaf48.png",
        "128": "/images/Leaf128.png"
    },
    "background.service_worker": {
        "scripts": [
            "socket.io.js"
        ]
    }
}